<div class="row">
  <div class="col-xs-4">
    <img src="{{unbound model.imagen}}">
  </div>
  <div class="col-xs-8">
    <div class="form-group">
      <input type="text" value="{{unbound model.nombres}}" class="form-control" placeholder="Nombres" />
    </div>
    <div class="form-group">
      <input type="email" value="{{unbound model.email}}" class="form-control" placeholder="Email" />
    </div>
  </div>
</div>
<div class="actions">
  <input type="submit" value="Actualizar" class="btn btn-primary">
</div>
